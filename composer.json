{
    "name": "czim/laravel-processor",
    "description": "Pipelined processor framework for Laravel.",
    "keywords": [
        "processor",
        "pipeline",
        "data processing",
        "modular processing",
        "process step"
    ],
    "homepage": "https://github.com/czim/laravel-processor",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pxlwidgets.com",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "czim/laravel-dataobject": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8|^4.0",
        "czim/laravel-repository": "*"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Processor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Processor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
