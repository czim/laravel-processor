{
    "name": "czim/laravel-processor",
    "description": "Pipelined processor framework for Laravel.",
    "keywords": [
        "processor",
        "pipeline",
        "data processing",
        "modular processing",
        "process step"
    ],
    "homepage": "https://github.com/czim/laravel-processor",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "http://www.pixelindustries.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "czim/laravel-dataobject": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Processor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Processor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
